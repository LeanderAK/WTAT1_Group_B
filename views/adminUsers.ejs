<style>
    table {
        border: solid 1px black;
    }

    th {
        border: solid 1px black;
        padding: 10px;
    }

    td {
        border: solid 1px black;
        padding: 10px;
    }
</style>
<div style="background-color: white; max-width: fit-content; margin: auto">
    <table>
        <tr><th>UserId</th><th>Username</th><th>Email</th><th>Password</th></tr>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user._id %></td>
                <td><%= user.username %></td>
                <td><%= user.email %></td>
                <td><%= user.password %></td>
            </tr>
        <% }) %>
    </table>
</div>